    },
    {
      "type": "range",
      "id": "section_padding_left_right_tablet",
      "label": "Section Left/Right Padding (Tablet)",
      "min": 10,
      "max": 100,
      "step": 5,
      "unit": "px",
      "default": 20
    },
    {
      "type": "range",
      "id": "section_padding_top_desktop",
