<div class="rh-learn_z-sub-plan-sec">
  <div class="container_rh">
    {% if section.settings.section_title != blank %}
      <div class="_rh-sub-plan-title">
        <span class="_rh-heading-white">{{ section.settings.section_title | newline_to_br }}</span>
      </div>
    {% endif %}
    {% if section.settings.section_description != blank %}
      <div class="_rh-sub-plan-des">
        {{ section.settings.section_description }}
      </div>
    {% endif %}
    {% if section.blocks.size > 0 %}
      <div class="_rh-sub-plan-cards">
        {% for block in section.blocks %}
          {% case block.type %}
            {% when 'plan_card' %}
              <div class="_rh-plan-card" {{ block.shopify_attributes }}>
                <div class="_rh-plan-card-header">
                  <div class="_rh-plan-card-header-content">
                    <div class="_rh-plan-card-title">{{ block.settings.plan_title | default: 'Plan Title' }}</div>
                    <div class="_rh-plan-card-subtitle">
                      <div class="_rh-plan-card-subtitle-text">
                        {{ block.settings.plan_subtitle | default: 'For you' }}
                      </div>
                      {% if block.settings.badge_text != blank %}
                        <div class="_rh-plan-card-subtitle-badge">
                          <div class="_rh-plan-card-icon">
                            <svg
                              width="14"
                              height="14"
                              viewBox="0 0 14 14"
                              fill="none"
                              xmlns="http://www.w3.org/2000/svg"
                            >
                              <use href="#user-icon"/>
                            </svg>
                          </div>
                          <div class="_rh-plan-card-badge-text">{{ block.settings.badge_text }}</div>
                        </div>
                      {% endif %}
                    </div>
                  </div>
                </div>
                <div class="_rh-plan-card-content">
                  <div class="_rh-plan-card-pricing">
                    <div class="_rh-plan-card-price-info">
                      <div class="_rh-plan-card-price">
                        {{ block.settings.price_text | default: 'Contact for pricing' }}
                      </div>
                      {% if block.settings.billing_text != blank %}
                        <div class="_rh-plan-card-billing">{{ block.settings.billing_text }}</div>
                      {% endif %}
                    </div>
                    {% if block.settings.cta_text != blank %}
                      <a href="{{ block.settings.cta_url | default: '#' }}" class="_rh-plan-card-button">
                        <div class="_rh-plan-card-button-content">
                          <div class="_rh-plan-card-button-text-container">
                            <div class="_rh-plan-card-button-text">{{ block.settings.cta_text }}</div>
                          </div>
                          <div class="_rh-plan-card-icon">
                            <svg
                              width="14"
                              height="14"
                              viewBox="0 0 14 14"
                              fill="none"
                              xmlns="http://www.w3.org/2000/svg"
                            >
                              <use href="#arrow-icon"/>
                            </svg>
                          </div>
                        </div>
                      </a>
                    {% endif %}
                  </div>
                  {% if block.settings.features != blank %}
                    <div class="_rh-plan-card-features">
                      {% assign features = block.settings.features | newline_to_br | split: '<br />' %}
                      {% for feature in features %}
                        {% if feature != blank %}
                          <div class="_rh-plan-card-feature">
                            <div class="_rh-plan-card-feature-icon">
                              <svg
                                width="15"
                                height="15"
                                viewBox="0 0 15 15"
                                fill="none"
                                xmlns="http://www.w3.org/2000/svg"
                              >
                                <use href="#checkmark-icon"/>
                              </svg>
                            </div>
                            <div class="_rh-plan-card-feature-text">{{ feature | strip }}</div>
                          </div>
                        {% endif %}
                      {% endfor %}
                    </div>
                  {% endif %}
                </div>
              </div>
          {% endcase %}
        {% endfor %}
      </div>
    {% endif %}
  </div>
</div>

{% style %}
      .rh-learn_z-sub-plan-sec {
    background: {{ section.settings.section_bg_color | default: '#000' }};
    padding-top: {{ section.settings.section_padding_top_mobile | default: 60 }}px;
    padding-bottom: {{ section.settings.section_padding_bottom_mobile | default: 60 }}px;
    padding-left: {{ section.settings.section_padding_left_right_mobile | default: 20 }}px;
    padding-right: {{ section.settings.section_padding_left_right_mobile | default: 20 }}px;
    min-height: 50vh;
  }

  .rh-learn_z-sub-plan-sec .container_rh {
    max-width: 1300px;
    margin: 0 auto;
    width: 100%;
    position: relative;
    z-index: 1;
  }

  .rh-learn_z-sub-plan-sec ._rh-sub-plan-title {
    width: 100%;
    text-align: center;
    display: flex;
    justify-content: center;
    flex-direction: column;
    margin: 0 0 30px;
  }

  ._rh-sub-plan-title ._rh-heading-white{
    background: radial-gradient(
      70.54% 70.54% at 50% 50%,
      #ffffff 0%,
      rgba(255, 255, 255, 0.573) 92.71%,
      rgba(255, 255, 255, 0.6) 92.72%
    );
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
    color: transparent;
    font-size: clamp(24px, 5vw, 48px);
    font-family: Inter, sans-serif;
    font-weight: 700;
    word-wrap: break-word;
    line-height: normal;
  }

  ._rh-sub-plan-des {
    font-family: Inter, sans-serif;
    font-weight: 400;
    font-size: 16px;
    line-height: 25.2px;
    text-align: center;
    color: #FFFFFF;
    margin: 0 0 50px;
  }

  /* Sub Plan Cards */
  ._rh-sub-plan-cards {
    width: 100%;
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 24px;
    max-width: 1300px;
    margin: 0 auto;
  }

  ._rh-plan-card {
    position: relative;
    background: linear-gradient(180deg, rgba(255, 255, 255, 0.03) 0%, rgba(153, 56, 151, 0.17) 100%);
    border-radius: 8px;
    border: 1px rgba(255, 255, 255, 0.25) solid;
    overflow: hidden;
    transition: transform 0.3s ease, box-shadow 0.3s ease;
  }

  ._rh-plan-card:hover {
    transform: translateY(-5px);
    box-shadow: 0 10px 30px rgba(158, 110, 229, 0.3);
  }

  ._rh-plan-card-header {
    background: rgba(163.45, 100.90, 255, 0.35);
    padding: 26px 35px;
    border-top-left-radius: 8px;
    border-top-right-radius: 8px;
  }

  ._rh-plan-card-header-content {
    display: flex;
    flex-direction: column;
    gap: 12px;
  }

  ._rh-plan-card-title {
    color: white;
    font-size: 28px;
    font-family: Inter, sans-serif;
    font-weight: 700;
    line-height: 24.30px;
    word-wrap: break-word;
  }

  ._rh-plan-card-subtitle {
    display: flex;
    align-items: center;
    gap: 5px;
    flex-wrap: wrap;
  }

  ._rh-plan-card-subtitle-text {
    color: white;
    font-size: 16px;
    font-family: Inter, sans-serif;
    font-weight: 400;
    line-height: 16.80px;
    word-wrap: break-word;
  }

  ._rh-plan-card-subtitle-badge {
    display: flex;
    align-items: center;
    gap: 5px;
  }

  ._rh-plan-card-icon {
    width: 20px;
    height: 20px;
    display: flex;
    justify-content: center;
    align-items: center;
  }

  ._rh-plan-card-icon-inner {
    width: 20px;
    height: 20px;
    position: absolute;
    top: 0;
    left: 0;
    overflow: hidden;
  }

  ._rh-plan-card-icon-shape {
    position: absolute;
    background: white;
  }

  ._rh-plan-card-icon-shape.user {
    width: 12.50px;
    height: 12.18px;
    left: 3.75px;
    top: 3.91px;
  }

  ._rh-plan-card-icon-shape.arrow {
    width: 12.19px;
    height: 11.99px;
    left: 3.75px;
    top: 4px;
  }

  ._rh-plan-card-badge-text {
    color: white;
    font-size: 16px;
    font-family: Inter, sans-serif;
    font-weight: 400;
    line-height: 19.60px;
    word-wrap: break-word;
  }

  ._rh-plan-card-content {
    padding: 35px;
    display: flex;
    flex-direction: column;
    gap: 25px;
  }

  ._rh-plan-card-pricing {
    display: flex;
    flex-direction: column;
    gap: 20px;
  }

  ._rh-plan-card-price-info {
    display: flex;
    flex-direction: column;
    gap: 10px;
  }

  ._rh-plan-card-price {
    color: white;
    font-size: 18px;
    font-family: Inter, sans-serif;
    font-weight: 700;
    line-height: 22.40px;
    word-wrap: break-word;
  }

  ._rh-plan-card-billing {
    color: white;
    font-size: 14px;
    font-family: Inter, sans-serif;
    font-weight: 400;
    line-height: 16.80px;
    word-wrap: break-word;
  }

  ._rh-plan-card-button {
    height: 48px;
    position: relative;
    background: linear-gradient(90deg, #9E6EE5 0%, #FA75F8 100%);
    border-radius: 4px;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor: pointer;
    transition: all 0.3s ease;
    text-decoration: none;
  }

  ._rh-plan-card-button:hover {
    background: linear-gradient(90deg, #b684f0 0%, #ff89fb 100%);
    transform: translateY(-2px);
    box-shadow: 0 5px 15px rgba(158, 110, 229, 0.4);
  }

  ._rh-plan-card-button-content {
    display: flex;
    align-items: center;
    gap: 5px;
  }

  ._rh-plan-card-button-text-container {
    position: relative;
    overflow: hidden;
  }

  ._rh-plan-card-button-text {
    color: white;
    font-size: 16px;
    font-family: Inter, sans-serif;
    font-weight: 600;
    line-height: 19.20px;
    word-wrap: break-word;
  }

  ._rh-plan-card-features {
    display: flex;
    flex-direction: column;
    gap: 10px;
  }

  ._rh-plan-card-feature {
    display: flex;
    align-items: center;
    gap: 10px;
  }

  ._rh-plan-card-feature-icon {
    position: relative;
    flex-shrink: 0;
  }

  ._rh-plan-card-feature-icon-inner {
    width: 17.15px;
    height: 21px;
    position: relative;
    overflow: hidden;
  }

  ._rh-plan-card-feature-icon-shape {
    width: 13.58px;
    height: 13.58px;
    position: absolute;
    left: 1.79px;
    top: 3.71px;
    background: white;
  }

  ._rh-plan-card-feature-text {
    color: white;
    font-size: 16px;
    font-family: Inter, sans-serif;
    font-weight: 400;
    line-height: 19.60px;
    word-wrap: break-word;
    flex: 1;
  }

  /* Responsive Design for Sub Plan Cards */
  @media (max-width: 480px) {
    ._rh-sub-plan-cards {
      grid-template-columns: 1fr;
      gap: 20px;
    }

    ._rh-plan-card-header {
      padding: 20px;
    }

    ._rh-plan-card-content {
      padding: 25px 20px;
    }

    ._rh-plan-card-title {
      font-size: 24px;
    }
  }

  @media (min-width: 481px) and (max-width: 768px) {
    ._rh-sub-plan-cards {
      grid-template-columns: 1fr;
      gap: 22px;
    }

    ._rh-plan-card-header {
      padding: 24px 30px;
    }

    ._rh-plan-card-content {
      padding: 30px;
    }
  }

  @media (min-width: 769px) and (max-width: 1024px) {
    ._rh-sub-plan-cards {
      grid-template-columns: repeat(2, 1fr);
      gap: 20px;
    }

    ._rh-plan-card:nth-child(3) {
      grid-column: 1 / -1;
      max-width: 50%;
      justify-self: center;
    }
  }

  /* Medium Mobile to Small Tablet (481px - 768px) */
  @media (min-width: 481px) and (max-width: 768px) {
    .rh-learn_z-sub-plan-sec {
      padding-top: {{ section.settings.section_padding_top_tablet | default: 50 }}px;
      padding-bottom: {{ section.settings.section_padding_bottom_tablet | default: 50 }}px;
      padding-left: {{ section.settings.section_padding_left_right_tablet | default: 20 }}px;
      padding-right: {{ section.settings.section_padding_left_right_tablet | default: 20 }}px;
    }
  }

  /* Large Tablet (769px - 1024px) */
  @media (min-width: 769px) and (max-width: 1024px) {
    .rh-learn_z-sub-plan-sec {
      padding-top: {{ section.settings.section_padding_top_tablet | default: 70 }}px;
      padding-bottom: {{ section.settings.section_padding_bottom_tablet | default: 70 }}px;
      padding-left: {{ section.settings.section_padding_left_right_tablet | default: 30 }}px;
      padding-right: {{ section.settings.section_padding_left_right_tablet | default: 30 }}px;
    }
  }

  /* Desktop (1025px and up) */
  @media (min-width: 1025px) {
    .rh-learn_z-sub-plan-sec {
      padding-top: {{ section.settings.section_padding_top_desktop | default: 100 }}px;
      padding-bottom: {{ section.settings.section_padding_bottom_desktop | default: 100 }}px;
      padding-left: {{ section.settings.section_padding_left_right_desktop | default: 20 }}px;
      padding-right: {{ section.settings.section_padding_left_right_desktop | default: 20 }}px;
    }

    ._rh-sub-plan-cards {
      grid-template-columns: repeat(3, 1fr);
      gap: 24px;
    }
  }
{% endstyle %}

